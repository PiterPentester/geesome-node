{
  "name": "geesome-node",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node ./index.ts",
    "recreate-database": "./bash/drop-database.sh && ./bash/create-database.sh",
    "migrate-database": "( cd ./components/database/mysql && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path ./components/database/mysql/migrations --name",
    "rollback-migration": "( cd ./components/database/mysql && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate:undo)",
    "upgrade": "./bash/upgrade-app.sh",
    "generate-docs": "./node_modules/.bin/apidoc -i components/api/http-v1 -o docs && ./node_modules/.bin/http-server ./docs",
    "recreate-test-database": "mysql -uroot -proot -e \"drop database test_geesome_core; create database test_geesome_core;\"",
    "test": "npm run recreate-test-database; rm -rf ./.jsipfs-test; ./node_modules/.bin/mocha --exit -r ts-node/register 'test/**/*.test.ts'",
    "testCoverage": "./node_modules/.bin/nyc -r lcov -e .ts -x \"*.test.ts\" ./node_modules/.bin/mocha -r ts-node/register test/**/*.test.ts && nyc report"
  },
